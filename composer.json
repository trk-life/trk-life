{
    "name": "trk-life/trk-life",
    "description": "A time tracker that you'll like.",
    "license": "MIT",
    "authors": [
        {
            "name": "George Webb",
            "email": "george@webb.uno"
        },
        {
            "name": "Joe Cotton",
            "email": "joe@hvy.io"
        }
    ],
    "require": {
        "php": "^5.5",
        "robmorgan/phinx": "^0.5.3",
        "symfony/yaml": "^3.0",
        "slim/slim": "^3.4",
        "monolog/monolog": "^1.19",
        "oscarotero/psr7-middlewares": "^3.15",
        "doctrine/orm": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4",
        "squizlabs/php_codesniffer": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "TrkLife\\": "app/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TrkLife\\": "app/tests/phpunit/"
        }
    },
    "config": {
        "vendor-dir": "app/vendor"
    }
}
